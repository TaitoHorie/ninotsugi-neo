
<div class="left-box">
  <div class="container">
    <%= form_with(model: @enban, local: true) do |form| %>
      <h3>
        編集する
      </h3>
      <%= form.text_field :title, placeholder: "Title" %>
      <%= form.text_field :genre, placeholder: "ジャンル" %>
      <%= form.text_field :name, placeholder: "ニックネーム" %>
      <%= form.text_field :image, placeholder: "Image Url" %>
      <%= form.submit "SEND" %>
    <% end %>
  </div>
</div>
